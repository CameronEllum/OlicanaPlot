{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:dev": "npm run lint && vite build --minify false --mode development",
    "build": "npm run lint && vite build --mode production",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "biome:check": "biome check --apply ."
  },
  "dependencies": {
    "@wailsio/runtime": "latest",
    "echarts": "^6.0.0",
    "plotly.js-dist-min": "^2.35.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.14",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@tsconfig/svelte": "^5.0.7",
    "svelte": "^5.47.1",
    "svelte-check": "^4.3.6",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  }
}